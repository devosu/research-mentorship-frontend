{
  "verbose": 2,
  "hooks": {
    "after:bump": [
    ],
    "after:release": [
    ]
  },
  "git": {
    "commit": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "header": "# Changelog",
      "preset": {
        "name": "conventionalcommits"
      }
    }
  }
}